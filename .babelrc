{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browser": [
            "last 2 Chrome versions",
            "last 2 Firefox versions",
            "last 2 Edge versions"
          ]
        }
      }
    ],
    "react"
  ],
  "plugins": [
    "react-hot-loader/babel",
    "transform-runtime",
    "transform-class-properties",
    "transform-object-rest-spread"
  ]
}
