{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browser": [
            "last 2 Chrome versions",
            "last 2 Firefox versions",
            "last 2 Edge versions"
          ]
        },
        "useBuiltIns": true
      }
    ],
    "react"
  ],
  "plugins": [
    "react-hot-loader/babel",
    ["transform-runtime", {"polyfill": false}],
    "transform-class-properties",
    ["transform-object-rest-spread", {"useBuiltIns": true}]
  ]
}
