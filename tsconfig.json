{
  "compilerOptions": {
    "lib": ["dom", "esnext"],
    "types": ["webpack-env", "node", "jest"],
    "resolveJsonModule": true,
    "allowJs": true,
    "esModuleInterop": true,
    "downlevelIteration": true,
    "noEmit": true,
    "jsx": "preserve",
    "skipLibCheck": true,
    "noUnusedLocals": true
  }
}
