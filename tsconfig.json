{
  "compilerOptions": {
    "lib": ["dom", "esnext"],
    "module": "es2022",
    "types": ["vite/client", "node", "jest", "react", "react-dom"],
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "allowJs": true,
    "esModuleInterop": true,
    "downlevelIteration": true,
    "noEmit": true,
    "jsx": "preserve",
    "skipLibCheck": true,
    "noUnusedLocals": true
  }
}
